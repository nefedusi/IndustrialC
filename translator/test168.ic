#define F_CPU 16000000L

proctype Blink(pin_num, on_time, off_time)
{
	state FS_START
	{
		set state On;
	}
	state On
	{
		set state Off;
	}
	state Off
	{
		set state On;
	}
}

int led1_on_time = 500;
int led1_off_time = 1000;
int led1_pin_num = 4;
int led2_on_time = 700;
int led2_off_time = 1400;
int led2_pin_num = 5;
Blink Led1_blink(led1_pin_num, led1_on_time, led1_off_time);
Blink Led2_blink(led2_pin_num, led1_on_time, led1_off_time);

process Init : background
{
	state FS_START
	{
		start process Led1_blink;
		start process Led2_blink;
		stop process;
	}
}
}





